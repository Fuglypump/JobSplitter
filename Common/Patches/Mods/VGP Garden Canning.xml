<?xml version="1.0" encoding="utf-8"?>

<Patch>
	<!-- VGP Garden Canning -->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>VGP Garden Canning</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/WorkGiverDef[defName = "Canning_WorkGiver_TakeProductOutOfUniversalFermenter"]/workType</xpath>
					<value>
						<workType>Butchering</workType>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/WorkGiverDef[defName = "Canning_WorkGiver_FillUniversalFermenter"]/workType</xpath>
					<value>
						<workType>Butchering</workType>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
