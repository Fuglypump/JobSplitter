<?xml version="1.0" encoding="utf-8"?>

<Patch>
  <!-- Veggie Mix -->
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Veggie Mix</li>
    </mods>
	<match Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/RecipeDef[defName = "VeggieMix5"]/requiredGiverWorkType</xpath>
				<value>
					<requiredGiverWorkType>Butchering</requiredGiverWorkType>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/RecipeDef[defName = "VeggieMixTwenty"]/requiredGiverWorkType</xpath>
				<value>
					<requiredGiverWorkType>Butchering</requiredGiverWorkType>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/RecipeDef[defName = "VeggieMix50"]/requiredGiverWorkType</xpath>
				<value>
					<requiredGiverWorkType>Butchering</requiredGiverWorkType>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/RecipeDef[defName = "VeggieMix75"]/requiredGiverWorkType</xpath>
				<value>
					<requiredGiverWorkType>Butchering</requiredGiverWorkType>
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>/Defs/RecipeDef[defName = "VeggieMix100"]/requiredGiverWorkType</xpath>
				<value>
					<requiredGiverWorkType>Butchering</requiredGiverWorkType>
				</value>
			</li>
		</operations>
	</match>	
  </Operation>
</Patch>
